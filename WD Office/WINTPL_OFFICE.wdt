     name : InitializeRibbon
     internal_properties : CAAAAAgAAAC5sNqRWiJqVuk+KJsqkXicTqOgnpAbZumqrY7DKaoiBQ0qDniFrP8igZe8/5VxmaCjyg2lf2e8wkPg+K9Dq8gjA+U2Iln5dq1RIqzTARm8yFrklOC/tOUs9fo690+G9Mfwns09Ow52+d4GwGiM0tvtK14X0FzpEQ2I6E7RvQJrHLWvPF+MiAIGhX9uUFWQOaJ9Vw3DfHLaF4os3r4+47qLxu1qEaOyzVPEAFdTwwEG08lwvblcPOTlGtvVX0fTpMmq4ly7ACVi
     procedure_id : 1898416325591829756
     type_code : 14
     starred : true
     code : |1-
      // Summary: Initializes the custom ribbon (mandatory after the initialization code of window)
      // Syntax:
      //InitializeRibbon (<sSourceRibbon> is string [, <oCustomRibbon> is control [, <pHideCallbackRibbon> is procedure]]) 
      //
      // Parameters:
      //	sSourceRibbon (ANSI string): Name of native ribbon
      //	oCustomRibbon (control - default value=0): Custom ribbon control
      // 	pHideCallbackRibbon (procedure - default value=0): Custom callback for hiding the ribbon
      // Return value:
      // 	None
      //
      
      Procdure InitializeRibbon(sSourceRibbon is string, oCustomRibbon is Control = Null, pHideCallbackRibbon is Procdure = Null)
      
      // Makes the window visible
      gDocumentWin..Visible = True
      
      // Checks the existence of source ribbon control
      if ControlExist(sSourceRibbon) = False then
      	
      	// It does not exist: it was customized or it is the standard toolbar, ignore
      	return
      end
      
      oNativeRibbon is champ <- {sSourceRibbon, indControl}
      
      // Applies the same style as the window
      oNativeRibbon..Image 	= gsImageRibbonPane
      oNativeRibbon..Style 	= RIBBON_STYLE..Style
      oNativeRibbon..Image 	= gsImageRibbonPane
      
      // Masks the first ribbon grouping (its features correspond to the file menu managed by the template)
      sGroupName is string = EnumControl(oNativeRibbon[1]..FullName, 1)
      if sGroupName <> "" then {sGroupName, indControl}..Visible = False
      
      // If a custom callback was supplied for hiding the ribbon
      if pHideCallbackRibbon <> Null then
      	// Calls the custom callback for hiding the ribbon
      	pHideCallbackRibbon( oNativeRibbon )
      end
      
      // Changes the background color
      oColor is a Color = STC_BCKGRD_TITLE_BAR..BrushColor
      oNativeRibbon..BrushColor = oColor
      
      // Generates the 5-state background image for the buttons
      with oColor
      	
      	oButtonImageStyle is an Image
      	oButtonImageStyle..Width = 100
      	oButtonImageStyle..Height = 20
      	
      	// Start the drawing
      	dStartDrawing(oButtonImageStyle, dWithOpacity)
      	
      	// Pressed
      	oColor = STC_BCKGRD_TITLE_BAR..BackgroundColor
      	..Saturation = 180
      	..Luminance = 95
      	..Opacity = 50
      	
      	dBackGround(..Color, BackgroundSolid, 0, ..Opacity)
      	dPen(Transparent)
      	dRectangle(20, 0, 39, 19)
      	
      	// Active
      	oColor = STC_BCKGRD_TITLE_BAR..BackgroundColor
      	..Saturation = 180
      	..Luminance = 115
      	..Opacity = 50
      	
      	dBackGround(..Color, BackgroundSolid, 0, ..Opacity)
      	dPen(Transparent)
      	dRectangle(60, 0, 79, 19)
      	
      	// Rollover
      	oColor = STC_BCKGRD_TITLE_BAR..BackgroundColor
      	..Saturation = 30
      	..Luminance = 160
      	..Opacity = 40
      	
      	dBackGround(..Color, BackgroundSolid, 0, ..Opacity)
      	dPen(Transparent)
      	dRectangle(80, 0, 99, 19)		
      	
      	// Save
      	gsButtonImageStyle = fDataDirUser() + [fSep] + "btn5.png"
      	dSaveImagePNG(oButtonImageStyle, gsButtonImageStyle)
      end
      
      // Applies the generated image to the declared buttons
      for each oControl of garrControlsStyle
      	if oControl = Null then continue
      	
      	// Applies the style
      	__ApplyStyle(oControl)
      end
      
      // Inserts the custom panes into the native ribbon: used to store the updates of "native" ribbon
      if oCustomRibbon <> Null then
      	RibbonDuplicatePanes(oCustomRibbon, oNativeRibbon)	
      end
      
      // Moves the UNDO/REDO buttons
      MoveUndoRedo()
      
      // Moves the read-only caption
      MoveReadOnly()
      
      end:	
      // The ribbon is updated every 250ms
      TimerSys(OnRibbonUpdate, 250ms)
     type : 458752
   -
     name : OnRibbonUpdate
     procedure_id : 1898416325591895292
     type_code : 14
     group : 5
     code : |1-
      // Summary: Update the ribbon <TO OVERLOAD IF NECESSARY>
      // Syntax:
      // OnRibbonUpdate ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      Procdure OnRibbonUpdate()
      
      // If the native controls exist
      if ControlExist("BTN_WDFAA_Redo") = True _and_ ControlExist("BTN_WDFAA_Undo") = True then
      	
      	extern BTN_WDFAA_Redo, BTN_WDFAA_Undo
      	
      	// Copies the status of buttons if they changed
      	if BTN_SHORT_REDO..State <> BTN_WDFAA_Redo..State then BTN_SHORT_REDO..State = BTN_WDFAA_Redo..State
      	if BTN_SHORT_UNDO..State <> BTN_WDFAA_Undo..State then BTN_SHORT_UNDO..State = BTN_WDFAA_Undo..State			
      end
     type : 458752
   -
     name : MenuListRecentDocuments
     procedure_id : 1898416325591960828
     type_code : 14
     code : |1-
      // Summary: Lists the documents recently opened
      // Syntax:
      //MenuListRecentDocuments ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      Procdure MenuListRecentDocuments()
      
      HourGlass( True )
      
      // Disables the display
      LOOP_RECENT_DOCUMENTS..DisplayEnabled = False
      BTN_OPEN_BROWSE..Visible = False
      
      // Signals the start of fill
      LOOP_RECENT_DOCUMENTS.StartFilling()
      
      // Clears the list
      LOOP_RECENT_DOCUMENTS.DeleteAll()
      
      // The MRU is already sorted
      FOR EACH stElement of SET_SETTING.MRU
      	
      	// If the file extension does not correspond to the one that was configured, ignore it
      	if Position(SET_SETTING.EXTENSION_DOCUMENT_PICKER, "*." + fExtractPath(stElement.FullPath, fExtension)[[2 to]], 1, IgnoreCase + WholeWord) < 1 then continue
      	
      	// If the document does not exist anymore, ignore it
      	if fFileExist(stElement.FullPath) = False then continue
      	
      	dtDate is datetime = DateTimeUTCToLocal( stElement.AccessDateTime )
      	
      	// Loads the file information of document				
      	nRow is int = LOOP_RECENT_DOCUMENTS.addline( stElement.FullPath, DateToString(dtDate..Date, maskDateRelativeDuration), *, stElement.DocPosition)
      	
      	LOOP_RECENT_DOCUMENTS[nRow].STC_DATE = dtDate
      	LOOP_RECENT_DOCUMENTS[nRow].STC_FOLDER = UncompleteDir(fExtractPath(stElement.FullPath, fDrive + fDirectory))
      	LOOP_RECENT_DOCUMENTS[nRow].STC_FILE_NAME = fExtractPath(stElement.FullPath, fFileName + fExtension)
      end
      
      // Signals the end of fill
      LOOP_RECENT_DOCUMENTS.EndFilling()
      LOOP_RECENT_DOCUMENTS.SelectMinus()
      
      // Re-enable the display
      BTN_OPEN_BROWSE..Visible = True
      LOOP_RECENT_DOCUMENTS..DisplayEnabled = True
      
      HourGlass( False )
     type : 458752
   -
     name : MenuOpenDocument
     internal_properties : CAAAAAgAAACm6X7eoSZQVwBFQzagMlNGmsZl2a8or+Dd92bWvB7hvdljJRj6quaRWpGJhDwvBG9MYSkLEzXHX4vvcIXS0pAD3rZolOP8rZO42x00ts6lVk2e2tmcOiLvoCZYgTVfnV9zkpL6u2/loU3ZNO0eZP8uANFP9uFgSj8GIDwUAduHNbC2
     procedure_id : 1898416325592026364
     type_code : 14
     code : |1-
      // Summary: Opens a document
      // Syntax:
      //MenuOpenDocument (<sPath> is string [, <bReuseWindow> is boolean [, <xDocPos> is numeric]])
      //
      // Parameters:
      //	sPath (ANSI string): Full document path
      //	bReuseWindow (boolean - default value=0): True to recycle the current window, False otherwise
      //	xDocPos (numeric - default value=0): Position saved in the document
      // Return value:
      // 	None
      //
      // Automatic process of exceptions:   run CASE EXCEPTION:
      //
      
      Procdure MenuOpenDocument(local sPath is string, bReuseWindow is boolean = False, local xDocPos is numeric = 0)
      
      sAlias is string
      bReadOnly is boolean = ( BTN_MENU_NEW..Visible = False _and_ BTN_MENU_SAVE..Visible = False )
      bFromExplorerDrop is boolean = ( _EVE.wMessage = 0x0233 ) // WM_DROPFILES
      bOpenNewWindow is boolean = False
      
      HourGlass( True )
      
      // If attempt to open the document currently opened
      if sPath ~= gsDocumentPath then
      	
      	// Closes the file menu
      	FileMenuClose( (not bOpenNewWindow) )
      	
      	// Abandon
      	return
      end
      
      // If no document is opened and if the creation is not enabled / or if a file was dropped at start / if a document was created without modifying it
      if bReuseWindow _or_ ( bFromExplorerDrop _and_ gsDocumentPath = "" ) _or_ ( gsDocumentPath = "" _and_ bReadOnly ) _or_ ( gsDocumentPath = "" _and_ gbModifiedDocument = False ) then
      	
      	// Open instead
      	sAlias = gDocumentWin..Alias = "" ? gDocumentWin..Name else gDocumentWin..Alias
      	
      	// If a file was dropped from the explorer or if the window is recycled
      	if bReuseWindow _or_ bFromExplorerDrop then
      		
      		// If the current document was not saved
      		if MenuCloseDocument( False ) = False then
      			
      			// Abandon
      			return
      		end
      	end		
      else
      	
      	sAlias = "DOC_" + SysDateTime		
      	
      	// Opens a window alias
      	OpenSister(sAlias + " = " + gDocumentWin..Name + NoFocus)
      	
      	// Maximizes the window if we are maximized
      	if WinSize(gDocumentWin) = MaxSize then WinSize(sAlias, MaxSize)
      	
      	// Store the fact that another window is opened
      	bOpenNewWindow = True
      end
      
      // Takes a reference on the alias actions
      pstDocumentActions is ACTION dynamic = {sAlias + ".DocumentAction", indVariable}
      
      // Opens the file
      if pstDocumentActions.OpenDocument = Null _or_ pstDocumentActions.OpenDocument( sPath, xDocPos ) = True then
      	
      	// Adds the file to the list of recent documents
      	SysRecentDocAdd( sPath )
      	
      	// Stores the document in the MRU
      	MRUAdd( sPath )
      	
      	// Loads the file information
      	ExecuteProcess(sAlias + ".MenuDisplayInfo", trtProcedure, ( sPath ))
      	
      	// Gives focus to the window
      	SysSetFocus( Handle(sAlias) )
      end
      
      // Closes the file menu
      FileMenuClose( (not bOpenNewWindow) )
      
      case exception:
      dbgAssert( False, ExceptionInfo )
      return
      
      end:
      
      HourGlass( False )
      
      if bFromExplorerDrop then
      	_EVE.Return = False
      end
