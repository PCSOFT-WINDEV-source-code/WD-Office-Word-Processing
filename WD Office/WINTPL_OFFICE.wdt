     name : MenuListRecentFolders
     procedure_id : 1898416325592354044
     type_code : 14
     code : |1-
      // Summary: Lists the recently used folders
      // Syntax:
      //MenuListRecentFolders ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      Procdure MenuListRecentFolders()
      
      arrFolders is array of strings
      
      HourGlass( True )
      
      // Disables the display
      LOOP_RECENT_LOCATIONS..DisplayEnabled = False
      BTN_SAVEAS_BROWSE..Visible = False
      
      // Signals the start of fill
      LOOP_RECENT_LOCATIONS.StartFilling()
      
      // Clears the list
      LOOP_RECENT_LOCATIONS.DeleteAll()
      
      // The MRU is already sorted
      FOR ALL ELEMENT stElement of SET_SETTING.MRU
      	
      	// If the file extension does not correspond to the one that was configured, ignore it
      	if Position(SET_SETTING.EXTENSION_DOCUMENT_PICKER, "*." + fExtractPath(stElement.FullPath, fExtension)[[2 to]], 1, IgnoreCase + WholeWord) < 1 then continue
      	
      	// If the document does not exist anymore, ignore it
      	if fFileExist(stElement.FullPath) = False then continue
      	
      	dtDate is datetime = DateTimeUTCToLocal( stElement.AccessDateTime )
      	
      	// Checks whether this folder was already found
      	if arrFolders.Seek(tccIgnoreCase, stElement.FullPath) > 0 then
      		continue
      	end
      	
      	// Stores the path
      	arrFolders.Add( stElement.FullPath )
      	
      	// Displays the folder
      	nRow is int = LOOP_RECENT_LOCATIONS.addline( stElement.FullPath, DateToString(dtDate..Date, maskDateRelativeDuration) )		
      	LOOP_RECENT_LOCATIONS[nRow].STC_FOLDER_NAME = UncompleteDir(fExtractPath(stElement.FullPath, fDrive + fDirectory))
      end	
      
      // Signals the end of fill
      LOOP_RECENT_LOCATIONS.EndFilling()
      LOOP_RECENT_LOCATIONS.SelectMinus()
      
      // Re-enable the display
      BTN_SAVEAS_BROWSE..Visible = True
      LOOP_RECENT_LOCATIONS..DisplayEnabled = True
      
      HourGlass( False )
