     type_code : 14
     code : |1-
      // Summary: Closes the File menu
      // Syntax:
      //FileMenuClose ([<bAnimation> is boolean])
      //
      // Parameters:
      // 	bAnimation (boolean - default value=1): <specify the role of bAnimation>
      // Return value:
      // 	None
      //
      Procdure FileMenuClose( bAnimation is boolean = True )
      
      pExecute is Procdure = BTN_MENU_BACK..Process[trtClick]
      
      // Triggers the process declared for the click on [Back] button
      // from BTN_FILE_MENU
      if pExecute <> Null then pExecute( bAnimation )
