      // Summary: Custom callback for hiding the ribbon
      // Parameters:
      //	oRibbon (control): Ribbon of WP control
      // Return value:
      // 	None
      //
      // 
      Procédure MaskGroupDocument(oRibbon is Control)
      
      // Hide the first 3 groupings of ribbon (New/Open, Save, Print/Export)
      
      MyWindow..DisplayEnabled = False
      
      // New/Open
      sGrouping is string = EnumControl( oRibbon, 1)
      if sGrouping <> "" then {sGrouping, indControl}..Visible = False
      
      // Save
      sGrouping = EnumControl( oRibbon, 2)
      if sGrouping <> "" then {sGrouping, indControl}..Visible = False
      
      // Print/Export
      sGrouping = EnumControl( oRibbon, 3)
      if sGrouping <> "" then {sGrouping, indControl}..Visible = False
      
      MyWindow..DisplayEnabled = True
