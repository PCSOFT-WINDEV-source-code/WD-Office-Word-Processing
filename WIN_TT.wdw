      // Code specific to creation
      DocumentAction.CreateDocument = __CreateNewTextDocument
      
      // Code specific to opening
      DocumentAction.OpenDocument = __OpenWP
      
      // Code specific to backup
      DocumentAction.SaveDocument = __SaveWP
      
      // SHIFT + F3: Uppercase characters 
      DeclareShortcut(skShift + VK_F3, _WP_ChangeUppercase)
      
      // CTRL + Space: Removes the style applied
      DeclareShortcut(skControl + VK_SPACE, _WP_InLight)
      
      
      	INTERNAL PROCEDURE __CreateNewTextDocument() : boolean
      	
      	// Reset the Word Processing control
      	WP_Main = ""
      	
      	result True
      	end
      
      	INTERNAL PROCEDURE __OpenWP( sPath is string ,  xDocPos is numeric = 0) : boolean
      	
      	// Loads the file in the Word Processing control
      	WP_Main.Ouvre(sPath)
      	
      	// Position of WP control
      	WP_Main.Positionne(xDocPos,xDocPos,30,40)
      	WP_Main..Cursor = xDocPos
      	WP_Main..CursorEnd = xDocPos
      	
      	result True
      	
      	end
      
      	INTERNAL PROCEDURE __SaveWP( sPath is string ) : boolean
      	
      	// Saves the file
      	result WP_Main.Sauve(sPath)
      	end
      
