     name : _WP_InLight
     procedure_id : 1898416286931675854
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      Procédure _WP_InLight()
      
      // Retrieves the current fragment
      FragmentStyle is a docFragment = __FragmentStyle()
      
      // Reset the style
      FragmentStyle..Formatting..FontItalic = False
      FragmentStyle..Formatting..FontBold = False
      FragmentStyle..Formatting..FontStrikeOut = False
      FragmentStyle..Formatting..HighlightColor = Transparent
      FragmentStyle..Formatting..TextColor = Black
      FragmentStyle..Formatting..VerticalAlign = alignBaseline
      
      // Gives focus back
      SetFocus(WP_Main)
      
      	// Summary: returns the fragment to use to apply a style
      	INTERNAL PROCEDURE __FragmentStyle()
      	FragmentCurrent is a docFragment <- WP_Main..Selection..Fragment
      	
      	// if no selection and if the cursor is positioned in a word, use the word
      	if WP_Main..SelectionLength=0
      		if FragmentCurrent..Word..Occurrence=1 then
      			result FragmentCurrent..Word[1]
      		end
      	end
      	result FragmentCurrent
      	end
     type : 458752
   -
     name : _WP_ChangeUppercase
     procedure_id : 1898416286931741390
     type_code : 14
     code : |1-
      // Summary: <specify the action of the procedure>
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      Procédure _WP_ChangeUppercase()
      
      // Retrieves the current fragment
      FragmentUppercase is a docFragment = __ExtractWord()
      
      // Temporary variable
      sText is string = FragmentUppercase..Text
      sFirstLetter is string = sText[1]
      
      switch True
      	// All lowercase => 1 letter in uppercase
      	case FragmentUppercase..Text = Lower(sText) 
      		FragmentUppercase..Text = Upper(sFirstLetter)  + (sText[2 to])
      		
      	// 1st letter uppercase => all in uppercase
      	case FragmentUppercase..Text = Upper(sFirstLetter) + Lower(sText[2 to])
      		FragmentUppercase..Text = Upper(sText)
      		
      	// All uppercase => all in lowercase + the other cases 
      	other case
      		// all in uppercase or all other cases
      		FragmentUppercase..Text = Lower(sText)
      end
      
      
      end:
      // Gives focus back
      SetFocus(WP_Main)
      
      
      	// Summary: returns the fragment to use to apply a style
      	INTERNAL PROCEDURE __ExtractWord()
      	FragmentCurrent is a docFragment <- WP_Main..Selection..Fragment
      	
      	// if no selection and if the cursor is positioned in a word, use the word
      	if WP_Main..SelectionLength=0
      		if FragmentCurrent..Word..Occurrence=1 then
      			result FragmentCurrent..Word[1]
      		end
      	end
      	result FragmentCurrent
      	end
