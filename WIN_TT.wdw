           type_code : 4
           p_codes :
            -
              code : |1-
               
               // If the association was not saved yet
               if LoadParameter("FileExtAssociation", "") = "" then
               	
               	// Implement the association
               	MySelf..Caption	= "Associate the files [%SET_SETTING.EXTENSION_DOCUMENT_PICKER%] to [%ProjetInfo(piNomProjet)%]"
               	MySelf..Note	= "1"
               	
               else
               	
               	// Cancel the association
               	MySelf..Caption	= "Cancel the association of files [%SET_SETTING.EXTENSION_DOCUMENT_PICKER%] to [%ProjetInfo(piNomProjet)%]"
               	MySelf..Note	= "0"
               end
               
               if InTestMode() = False then MySelf..ToolTip = MySelf..Caption
               MySelf..Width = TextWidth(MySelf, MySelf..Caption, tSingleLine) + CoordinateEditorToScreen(24 + 8)
               
               // If the UAC is active and if the user is not administrator
               if UACActive() _and_ UserIsAdministrator() = False then
               	
               	MySelf..Image = IMG_UAC
               end
              type : 14
            -
              internal_properties : CAAAAAgAAABfGQ6FITMFEiuwex5ACwhJx1H0AIhxs59bVldMpKH3n2WQRlQjrOFxXA//QFxqLQ7bLvKs4Dh0tvQrmi8mOshiGzNWhSNOwrIavFSu3KySGGnr2v1ghcAxps7XLjXTYVJD98FTMDwqPjCCo/g=
              code : |1-
               
               bAssociation is boolean = Val(MySelf..Note)
               
               // Associates the extensions declared to the program
               if AssociateExtensions( bAssociation ) = False then
               	// The start was canceled
               	return
               end
               
               if bAssociation then
               	
               	// Marks that the association was already performed
               	SaveParameter("FileExtAssociation", "Done")
               	
               	// Displays a toast
               	ToastDisplay("The files [%SET_SETTING.EXTENSION_DOCUMENT_PICKER%] will now be opened with [%ProjetInfo(piNomProjet)%]", toastShort, vaMiddle)
               	
               else
               	
               	// Cancels the mark indicating that the association was performed
               	SaveParameter("FileExtAssociation", "")
               	
               	// Displays a toast
               	ToastDisplay("The files [%SET_SETTING.EXTENSION_DOCUMENT_PICKER%] will no longer be opened with [%ProjetInfo(piNomProjet)%]", toastShort, vaMiddle)
               end
               
               // Update the button
               MySelf.ExecuteProcess( trtInit )
               
               // Closes the menu
               FileMenuClose()
              type : 18
            -
